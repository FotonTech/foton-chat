{
  "extends": "airbnb/base",
  "parser": "babel-eslint",
  "rules": {
    "no-console": 0,
    "max-len": [1, 120, 2],
    "no-param-reassign": [2, { "props": false }],
    "no-continue": 0,
    "no-underscore-dangle": 0,
    "generator-star-spacing": 0,
    "import/no-named-as-default": 0,
    "no-return-await": 0,
    "no-shadow": 0,
    "object-curly-newline": 0,
    "import/prefer-default-export": 0,
  },
  "env": {
    "jest": true
  }
}
